{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "importHelpers": true,
    "declaration": false,
    "outDir": "lib",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "DOM",
      "ES6"
    ],
    "strict": true,
    "skipLibCheck": true,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false
  },
  "include": [
    "**/*.ts"
  ]
}